pipeline {
  agent any
  tools { 
    maven 'maven 3.6.3' 
  }
  stages { 
      stage ("Build") {
      steps {
	    sh "mvn clean install" 
      }  
     }
     
      stage ("Code Analysis") {
      steps {
	    withSonarQubeEnv('SonarQube') {
	         sh "mvn sonar:sonar -Dsonar.projectKey=TestNGDemo1"
		 }
       }
     }
     
      stage ("Final") {
        steps {
        echo "Deployment Successfull"
        }
     }
     
  }
}
